opam-version: "2.0"
name: "color"
version: "0.1"
synopsis: "Library for working with colors. Conversion between RGB and HSL"
maintainer: "Anurag Soni <anuragsoni.13@gmail.com>"
authors: "Anurag Soni <anuragsoni.13@gmail.com>"
license: "MIT"
build: [
  ["jbuilder" "subst"] {pinned}
  ["jbuilder" "build" "-p" name "-j" jobs]
]
build-test: ["jbuilder" "runtest" "-p" name]

depends: [
  "jbuilder" {build & >= "1.0+beta14"}
  "alcotest" {test}
]
